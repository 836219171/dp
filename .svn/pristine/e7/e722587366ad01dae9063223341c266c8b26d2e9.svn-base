html,
body {
  margin: 0;
  padding: 0;
  width: 100%;
  height: 100%;
}
html .city,
body .city {
  margin: 0 auto;
  width: 100%;
  height: 190px;
  overflow: hidden;
}
html .city .cityData,
body .city .cityData {
  position: relative;
  margin: 8px auto;
  width: 96%;
  height: 100%;
}
html .city .cityData table,
body .city .cityData table {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: auto;
}
html .city .cityData table tr,
body .city .cityData table tr {
  width: 100%;
  height: 45px;
}
html .city .cityData table tr td,
body .city .cityData table tr td {
  min-width: 120px;
  height: 45px;
  line-height: 45px;
  text-align: center;
  font-size: 18px;
  color: #fff;
}
html .city .cityData table tr td:nth-child(1),
body .city .cityData table tr td:nth-child(1) {
  font-family: "impact";
  color: #10f6e9;
}
html .city .cityData table tr td:nth-child(3),
body .city .cityData table tr td:nth-child(3) {
  font-family: "impact";
  color: #10f6e9;
}
html .city .cityData table tr:nth-child(odd),
body .city .cityData table tr:nth-child(odd) {
  background: #123a72;
}

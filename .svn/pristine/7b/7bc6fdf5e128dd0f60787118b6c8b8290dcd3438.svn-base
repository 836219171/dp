.pies {
  position: relative;
  margin-top: 11px;
  width: 100%;
  height: 188px;
  box-sizing: border-box;
}
.pies #dyfwjc-pie {
  position: absolute;
  top: 3.5px;
  left: 4px;
  width: 100%;
  height: 175px;
}
.pies .warp {
  position: absolute;
  top: 0;
  left: 36px;
  width: 175px;
  height: 175px;
  background: url('../../../img/bg9.png') no-repeat center center;
  animation: warp 6s infinite linear;
  background-size: 100% 100%;
}
.pies .inner {
  position: absolute;
  top: 17px;
  left: 54px;
  width: 140px;
  height: 140px;
  background: url('../../../img/bg11.png') no-repeat center center;
  background-size: 100% 100%;
}
.pies .jiaDian {
  position: absolute;
  top: 18px;
  left: 96px;
  width: 2px;
  height: 11px;
  transform-origin: -9px 70px;
  background: url('../../../img/bg12.png') no-repeat center center;
}
@keyframes warp {
  from {
    transform: rotate(0deg);
  }
  to {
    transform: rotate(-360deg);
  }
}

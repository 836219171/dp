html,
body {
  margin: 0;
  padding: 0;
  width: 100%;
  height: 100%;
  background: url("../img/index-bg.png") no-repeat;
}
.odr-index {
  position: fixed;
  top: 0;
  left: 0;
  width: 1920px;
  height: 1080px;
  box-sizing: border-box;
}
.odr-index .odr-top {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 83px;
  background: url('../img/logo.png') no-repeat;
}
.odr-index .odr-main {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 1000px;
}
.odr-index .odr-main .odr-left,
.odr-index .odr-main .odr-middle,
.odr-index .odr-main .odr-right {
  display: inline-block;
  vertical-align: top;
}
.odr-index .odr-main .odr-left {
  padding-left: 55px;
  width: 486px;
}
.odr-index .odr-main .odr-left .ul-content .li-content {
  position: relative;
  display: block;
  margin-top: 11px;
  width: 100%;
  height: 228px;
  background: url('../img/icon-pos-b.png') no-repeat;
}
.odr-index .odr-main .odr-left .ul-content .li-content .li-content-title {
  display: inherit;
  padding-top: 8px;
  text-align: center;
  font-size: 16px;
  color: #fff;
  font-weight: 600;
  text-shadow: 0 3px 5px rgba(5, 24, 53, 0.5);
  background-image: -webkit-gradient(linear, 0 0, 0 bottom, from(#fff), to(#e5fcfd));
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}
.odr-index .odr-main .odr-left .ul-content .li-content .container {
  position: absolute;
  left: 4px;
  bottom: 4px;
  width: 478px;
  height: 190px;
}
.odr-index .odr-main .odr-middle {
  position: relative;
  width: 826px;
  height: 1000px;
}
.odr-index .odr-main .odr-middle .odr-middle-top {
  margin-top: 15px;
  width: 100%;
  height: 700px;
}
.odr-index .odr-main .odr-middle .odr-middle-bottom {
  position: absolute;
  left: 0;
  bottom: 46px;
  width: 100%;
  height: 226px;
  background: url('../img/icon-pos-d.png') no-repeat;
}
.odr-index .odr-main .odr-middle .odr-middle-bottom .li-content-title {
  display: inherit;
  padding-top: 8px;
  text-align: center;
  font-size: 16px;
  color: #fff;
  font-weight: 600;
  text-shadow: 0 3px 5px rgba(5, 24, 53, 0.5);
  background-image: -webkit-gradient(linear, 0 0, 0 bottom, from(#fff), to(#e5fcfd));
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}
.odr-index .odr-main .odr-middle .odr-middle-bottom .container {
  position: absolute;
  left: 4px;
  bottom: 4px;
  width: 818px;
  height: 187px;
}
.odr-index .odr-main .odr-right {
  width: 486px;
  height: 1000px;
}
.odr-index .odr-main .odr-right .ul-content {
  padding-top: 1px;
}
.odr-index .odr-main .odr-right .ul-content .li-content {
  display: block;
  margin-top: 4px;
  width: 486px;
  height: 119px;
  cursor: pointer;
  overflow: hidden;
  transition: all .5s;
  background: url('../img/bg-right-li.png') no-repeat;
}
.odr-index .odr-main .odr-right .ul-content .li-content .li-content-title span {
  position: relative;
  display: inline-block;
  vertical-align: middle;
  padding-top: 22px;
  width: 240px;
  font-size: 18px;
  color: #fff;
  text-indent: 3.4em;
  background: url('../img/icon-quan.png') no-repeat 40px 29px;
  text-shadow: 0 4px 6px rgba(0, 0, 0, 0.99);
}
.odr-index .odr-main .odr-right .ul-content .li-content .li-content-title span a {
  position: absolute;
  display: inline-block;
  vertical-align: middle;
  line-height: 25px;
  font-size: 14px;
  font-family: 'quartz';
}
.odr-index .odr-main .odr-right .ul-content .li-content .stat-data {
  position: relative;
}
.odr-index .odr-main .odr-right .ul-content .li-content .stat-data ul {
  display: inline-block;
  vertical-align: middle;
  padding: 7px 30px 0 35px;
  font-size: 0;
}
.odr-index .odr-main .odr-right .ul-content .li-content .stat-data ul li {
  display: inline-block;
  vertical-align: middle;
  width: 35px;
  height: 55px;
  font-size: 35px;
  font-family: 'quartz';
  color: #fff;
  text-align: center;
  background: url('../img/icon-nuber.png') no-repeat -1px 3px;
  background-size: 88px;
}
.odr-index .odr-main .odr-right .ul-content .li-content .stat-data .stat-user {
  position: absolute;
  top: -8px;
  right: 20px;
}
.odr-index .odr-main .odr-right .ul-content .li-content .stat-data .stat-user .stat-user-detail {
  display: inline-block;
  vertical-align: middle;
  width: 110px;
  height: 60px;
}
.odr-index .odr-main .odr-right .ul-content .li-content .stat-data .stat-user .stat-user-detail .stat-user-img {
  display: inline-block;
  vertical-align: middle;
}
.odr-index .odr-main .odr-right .ul-content .li-content .stat-data .stat-user .stat-user-detail .stat-user-img i {
  display: block;
  width: 35px;
  height: 40px;
  background: url('../img/icon-user.png') no-repeat;
}
.odr-index .odr-main .odr-right .ul-content .li-content .stat-data .stat-user .stat-user-detail .stat-user-img span {
  padding-left: 0.2em;
  font-size: 14px;
  color: #fff;
}
.odr-index .odr-main .odr-right .ul-content .li-content .stat-data .stat-user .stat-user-detail .stat-user-span {
  display: inline-block;
  vertical-align: middle;
  font-size: 30px;
  color: #10efe4;
}
.odr-index .odr-main .odr-right .ul-content .li-content .stat-data .stat-user .stat-user-nv .stat-user-img i {
  background: url('../img/icon-user.png') no-repeat 0px -56px;
}
.odr-index .odr-main .odr-right .ul-content .li-content .stat-data .stat-user .stat-user-nv .stat-user-span {
  color: #3098f6;
}
.odr-index .odr-main .odr-right .ul-content .li-content .stat-sfqr,
.odr-index .odr-main .odr-right .ul-content .li-content .stat-sqgs {
  display: inline-block;
  vertical-align: middle;
  margin: 0 39px;
  margin-top: -10px;
  width: 145px;
  height: 65px;
  background: url('../img/bg-sfqr.png') no-repeat;
}
.odr-index .odr-main .odr-right .ul-content .li-content .stat-sfqr span,
.odr-index .odr-main .odr-right .ul-content .li-content .stat-sqgs span {
  display: inline-block;
  margin: 5px 0 0 20px;
  vertical-align: middle;
  width: 100%;
  height: 65px;
  line-height: 65px;
  text-align: center;
  font-size: 30px;
  font-family: 'quartz';
  color: #fff;
}
.odr-index .odr-main .odr-right .ul-content .li-content .stat-sqgs {
  margin-left: 62px;
  background: url('../img/bg-sfqr.png') no-repeat 0 -108px;
}
.odr-index .odr-main .odr-right .ul-content .li-content .container-list .container-title {
  position: relative;
  margin-top: 25px;
  width: 100%;
  height: 40px;
  font-size: 16px;
  color: #fff;
  text-align: center;
  line-height: 33px;
  background: url('../img/right-title-bg.png') no-repeat center;
}
.odr-index .odr-main .odr-right .ul-content .li-content .container-list .container-title span {
  font-weight: 600;
  text-shadow: 0 3px 5px rgba(5, 24, 53, 0.5);
  background-image: -webkit-gradient(linear, 0 0, 0 bottom, from(#fff), to(#e5fcfd));
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}
.odr-index .odr-main .odr-right .ul-content .li-content .container-list .container-title i {
  position: absolute;
  top: 0;
  left: 20px;
  display: inline-block;
  width: 40px;
  height: 40px;
  background: url('../img/left-blue-off.png') no-repeat center;
  background-size: 100% 100%;
}
.odr-index .odr-main .odr-right .ul-content .li-content .container-list .container-title i:hover {
  background: url('../img/left-blue-on.png') no-repeat center;
  background-size: 100% 100%;
}
.odr-index .odr-main .odr-right .ul-content .li-content .container-list .container-title em {
  position: absolute;
  top: 0;
  right: 18px;
  display: inline-block;
  width: 40px;
  height: 40px;
  background: url('../img/right-blue-off.png') no-repeat center;
  background-size: 100% 100%;
}
.odr-index .odr-main .odr-right .ul-content .li-content .container-list .container-title em:hover {
  background: url('../img/right-blue-on.png') no-repeat center;
  background-size: 100% 100%;
}
.odr-index .odr-main .odr-right .ul-content .li-content .container {
  margin-left: 11px;
  width: 467px;
  height: 284px;
}
.odr-index .odr-main .odr-right .ul-content .active {
  height: 465px;
  background: url('../img/bg-right-li-d.png') no-repeat;
}

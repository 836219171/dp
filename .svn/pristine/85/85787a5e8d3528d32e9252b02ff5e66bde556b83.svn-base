html,
body {
    margin: 0;
    padding: 0;
    width: 100%;
    height: 100%;
    .city {
        margin: 0 auto;
        width: 100%;
        height: 190px;
        // background: #113260;
        overflow: hidden;
        .cityData {
            position: relative;
            margin: 8px auto;
            width: 96%;
            height: 100%;
            table {
                position: absolute;
                top: 0;
                left: 0;
                width:100%;
                height: auto;
                tr{
                    width: 100%;
                    height: 45px;
                    td {
                        min-width: 120px;
                        height: 45px;
                        line-height: 45px;
                        text-align: center;
                        font-size: 18px;
                        color: #fff;
                        
                    }
                    td:nth-child(1) {
                        font-family: "impact";
                        color:#10f6e9;
                    }
                    td:nth-child(3) {
                        font-family: "impact";
                        color:#10f6e9;
                    }
                }
                tr:nth-child(odd){
                    // background: #113266;
                    background: #123a72;
                }
            }
        }  
    }
}
